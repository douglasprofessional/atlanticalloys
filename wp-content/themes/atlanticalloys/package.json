{
  "name": "wp-theme",
  "version": "1.0.1",
  "description": "Wordpress Start Kit Theme",
  "main": "./assets/js/main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development SERVER=true webpack --watch",
    "watch": "cross-env NODE_ENV=development webpack --watch",
    "dev": "cross-env NODE_ENV=development webpack",
    "prod": "cross-env NODE_ENV=production webpack",
    "build": "concurrently \"yarn prod\" \"yarn dev\"",
    "lint-test": "vendor/bin/phpcs / --ignore=node_modules/,vendor/",
    "lint": "phpcs --standard=phpcs.xml -s ./ --ignore=node_modules/,vendor/",
    "fix-lint": "phpcbf --standard=phpcs.xml ./ --ignore=node_modules/,vendor/ --extensions=php"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.10",
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "autoprefixer": "^8.4.1",
    "babel-loader": "^8.2.2",
    "browser-sync": "^2.26.13",
    "browser-sync-webpack-plugin": "^2.3.0",
    "concurrently": "^3.6.1",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^5.2.1",
    "css-loader": "^6.8.1",
    "cssnano": "^4.1.10",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jquery": "^1.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.24",
    "postcss-custom-properties": "^8.0.11",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^7.3.3",
    "postcss-nested": "^6.0.1",
    "postcss-preset-env": "^8.5.0",
    "postcss-scss": "^2.1.1",
    "purgecss-with-wordpress": "^1.1.0",
    "regenerator": "^0.14.7",
    "resolve-url-loader": "^2.3.2",
    "sass": "^1.30.0",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "tailwindcss": "^3.3.2",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 11"
  ],
  "author": {
    "name": "Nucleus",
    "homepage": "https://nucleus.eti.br/"
  },
  "license": "GPL-2.0+",
  "dependencies": {
    "aos": "^2.3.4",
    "owl.carousel": "^2.3.4",
    "purgecss-with-gutenberg": "^1.0.2"
  }
}
